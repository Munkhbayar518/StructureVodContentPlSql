--  DDL for Trigger TR_TV_CONTENT_GENRES
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "UNI_DISH"."TR_TV_CONTENT_GENRES" 
BEFORE INSERT ON TV_CONTENT_GENRES FOR EACH ROW
